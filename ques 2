#include <stdio.h>

int main() {
    int num1, num2, hcf;

    // Input two numbers
    printf("Enter two integers: ");
    scanf("%d %d", &num1, &num2);

    // Find HCF using iterative method
    for (int i = 1; i <= num1 && i <= num2; i++) {
        // Check if i divides both numbers
        if (num1 % i == 0 && num2 % i == 0) {
            hcf = i;  // update HCF
        }
    }

    printf("The HCF (GCD) of %d and %d is: %d\n", num1, num2, hcf);
    return 0;
}
